    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.trebovanje && $data.trebovanje_proizvodi ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.trebovanje.proizvodi = $data.trebovanje_proizvodi;",
        "\treturn {",
        "\t\t//map trebovanje variable to expected task contract input",
        "\t\tkreriranoTrebovanjeInput: {",
