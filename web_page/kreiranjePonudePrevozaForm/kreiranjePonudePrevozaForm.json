    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.ponudaPrevoz ){",
        "\treturn {",
        "\t\t//map ponudaPrevoz variable to expected task contract input",
        "\t\tponudaPrevozInput: {",
        "\t\t\topcije: $data.ponudaPrevoz.opcije.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tnaziv: it.naziv !== undefined ? it.naziv : null,",
        "\t\t\t\tcena: it.cena !== undefined ? it.cena : null,",
        "\t\t\t\todabran: it.odabran !== undefined ? it.odabran : null",
        "\t\t\t}))",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
